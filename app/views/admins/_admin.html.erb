<tr id="<%= dom_id(admin) %>">
  <td class="id"><%= admin.id %></td>
  <td class="width-2em"><%= admin.email %></td>
  <td class="width-2em"><%= admin.roles %></td>
  <td class="width-2em"><%= admin.sign_in_count %></td>
  <td class="width-2em"><%= admin.current_sign_in_ip %></td>
  <td class="width-2em"><%= admin.created_at %></td>
  <td class="width-2em center"><%= lock_link_for admin %></td>
  <td class="width-2em center"><%= link_to '删除', admin, remote: true, method: :delete, data: { confirm: '确定要删除该用户吗？'} %></td>
</tr>